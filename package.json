{
  "name": "shares-help",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:all": "run-p start:notify start",
    "watch:all": "run-p watch:notify watch",
    "start": "cross-env NODE_ENV=develop CONFIG_FILE_NAME='config.dev.yml' nest start node-qaunt",
    "watch": "cross-env NODE_ENV=develop CONFIG_FILE_NAME='config.dev.yml' nest start node-qaunt --watch",
    "start:data": "cross-env NODE_ENV=develop CONFIG_FILE_NAME='config.dev.yml' nest start service-data",
    "watch:data": "cross-env NODE_ENV=develop CONFIG_FILE_NAME='config.dev.yml' nest start service-data --watch",
    "start:notify": "cross-env NODE_ENV=develop CONFIG_FILE_NAME='config.dev.yml' nest start service-notify",
    "watch:notify": "cross-env NODE_ENV=develop CONFIG_FILE_NAME='config.dev.yml' nest start service-notify --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=prod CONFIG_FILE_NAME='config.prod.yml' node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/service-core/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/microservices": "^8.1.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.0.2",
    "axios": "^0.21.4",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "camelcase": "^6.2.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dayjs": "^1.10.7",
    "ejs": "^3.1.6",
    "helmet": "^4.6.0",
    "js-yaml": "^4.1.0",
    "mysql2": "^2.3.0",
    "nest-winston": "^1.5.0",
    "nodemailer": "^6.6.3",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.37",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "xlsx": "^0.17.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
